{
  "color": {
    "alternate": {
      "base": {
        "background": { "value": "{alias.color.scheme.base.background.value}" },
        "foreground": { "value": "{alias.color.scheme.base.foreground.value}" },
        "dim": { "value": "{alias.color.scheme.base.dim.value}" }
      },
      "inverse": {
        "background": { "value": "color.base.foreground.value}" },
        "foreground": { "value": "color.base.background.value}" }
      },
      "surface": {
        "background": {
          "value": "{alias.color.scheme.surface.background.value}"
        },
        "foreground": {
          "value": "{alias.color.scheme.surface.foreground.value}"
        }
      }
    }
  }
}
